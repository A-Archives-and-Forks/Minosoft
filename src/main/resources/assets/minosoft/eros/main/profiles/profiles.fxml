            </rowConstraints>
            <GridPane.margin>
                <Insets right="10.0"/>
            </GridPane.margin>
            <ListView fx:id="managerTypeListViewFX" maxHeight="Infinity" maxWidth="Infinity" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"/>
        </GridPane>
    </GridPane>
